# Logo SQL Server bağlantı bilgileri
# Bu dosyayı .env olarak kopyalayın ve gerçek değerlerle doldurun

LOGO_SQL_SERVER=your_server_address,1433
LOGO_SQL_DB=logo
LOGO_SQL_USER=your_username
LOGO_SQL_PASSWORD=your_password

# Opsiyonel: Firma ve dönem numaraları
LOGO_COMPANY_NR=025
LOGO_PERIOD_NR=01

# Opsiyonel: ODBC Driver zorla belirtmek için
# LOGO_SQL_DRIVER=ODBC Driver 17 for SQL Server